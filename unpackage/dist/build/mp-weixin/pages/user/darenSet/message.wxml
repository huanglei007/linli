<view><view class="form"><view class="item flexd flex-center"><view class="label">达人名称</view><input class="input" type="text" placeholder="{{'请输入'+(userInfo.user_type==3?'店铺':'达人')+'名称'}}" placeholder-style="color:#878787" data-event-opts="{{[['input',[['__set_model',['$0','talent_name','$event',[]],['detail']]]]]}}" value="{{detail.talent_name}}" bindinput="__e"/></view></view><view class="form"><view class="item flexd flex-center"><view class="label">达人地址</view><input class="input" type="text" placeholder="{{'请输入'+(userInfo.user_type==3?'商家':'达人')+'地址'}}" placeholder-style="color:#878787" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['detail']]]]]}}" value="{{detail.address}}" bindinput="__e"/></view></view><view class="picBox"><view class="title">服务时间</view><view class="flexd flex-center jubetween"><view class="item"><time-picker vue-id="78186962-1" star="{{starTime}}" data-event-opts="{{[['^change',[['bindTimeChange']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-input">{{detail.service_begin_time}}</view></time-picker></view><view class="item"><time-picker vue-id="78186962-2" star="{{starTime2}}" data-event-opts="{{[['^change',[['bindTimeChange2']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-input">{{detail.service_end_time}}</view></time-picker></view></view></view><view class="form"><view class="item flexd flex-center"><view class="label">联系电话</view><input class="input" type="number" placeholder="请输入联系方式" placeholder-style="color:#878787" data-event-opts="{{[['input',[['__set_model',['$0','talent_phone','$event',[]],['detail']]]]]}}" value="{{detail.talent_phone}}" bindinput="__e"/></view></view><view class="form"><view class="item flexd flex-center"><text class="label">分类</text><picker value="{{classIndex}}" range="{{classArray}}" range-key="service_type_name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><text style="color:rgb(135, 135, 135);">{{classArray[classIndex]?classArray[classIndex].service_type_name:''}}</text></picker></view></view><view class="foot"><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn" bindtap="__e">提交审核</view></view></view>