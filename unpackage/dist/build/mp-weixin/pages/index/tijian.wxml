<view class="content"><view class="head"><view class="homeBg">="imageurl+'static/image/yytop.png'" mode="widthFix"></view><view data-event-opts="{{[['tap',[['$jumpback']]]]}}" class="backs" style="{{'top:'+(htosp+'px')+';'}}" bindtap="__e"><image src="/static/image/icon_fh.png" mode="widthFix"></image></view></view><view class="order"><view class="input"><input type="text" placeholder="请输入预约人姓名" placeholder-style="color: #8b8b8b" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="input"><input type="text" placeholder="请输入联系方式" placeholder-style="color: #8b8b8b" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="input"><picker class="picker" value="{{classfyIndex}}" range="{{classfy}}" range-key="project_name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{classfy[classfyIndex].project_name}}</view></picker></view><view class="input"><picker mode="date" fields="day" start="{{startDate}}" end="{{endDate}}" value="{{date}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{date==''?'请选择预约时间':date}}</view></picker></view><view class="input"><input type="text" placeholder="请填写体检地址" placeholder-style="color: #8b8b8b" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['submit']]]]}}" class="submit" bindtap="__e">提交</view></view></view>