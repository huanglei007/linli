<view><view class="login_title">注册</view><view class="item"><label>账 号:</label><input type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="msg_box"><label>验证码:</label><input placeholder="请输入验证码~" data-event-opts="{{[['input',[['__set_model',['','msg','$event',[]]]]]]}}" value="{{msg}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getMsg',['$event']]]]]}}" bindtap="__e">{{msgBoxShow[checkIndex]+(checkIndex==2?'s':'')}}</view></view><view class="item"><label>密 码:</label><input type="password" placeholder="请输入注册的密码" data-event-opts="{{[['input',[['__set_model',['','pass','$event',[]]]]]]}}" value="{{pass}}" bindinput="__e"/></view><view class="item"><label>确认密码:</label><input type="password" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','affirmPassword','$event',[]]]]]]}}" value="{{affirmPassword}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['saveUserInfo',['$event']]]]]}}" class="btn_box" bindtap="__e">注册</view><view data-event-opts="{{[['tap',[['getCheckRule',['$event']]]]]}}" class="rule_box" bindtap="__e"><view class="{{[checkRule?'cehck':'']}}"></view><view>请同意<label data-event-opts="{{[['tap',[['getRuleInfomation',['$event']]]]]}}" catchtap="__e">《隐私协议》</label>和<label>《用户协议》</label></view></view></view>