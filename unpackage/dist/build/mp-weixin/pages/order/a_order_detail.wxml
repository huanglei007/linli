<view class="order_detail-box"><view class="type-box"><view class="type-box-text flexd jubetween"><view class="text-title"><text class="font32">{{form.categoryName}}</text></view><view class="text-price fontColor-FF6000"><text class="font32">￥</text><text class="font42">{{form.commission}}</text></view></view><view class="type-box-upTime flexd jubetween"><view class="upTime-title"><text class="font28 fontColor-999">上门时间</text></view><view class="upTime-time font28"><uni-dateformat vue-id="5157d26c-1" date="{{form.start_time}}" format="MM-dd hh:mm " bind:__l="__l"></uni-dateformat><text>至</text><uni-dateformat vue-id="5157d26c-2" date="{{form.end_time}}" format=" MM-dd hh:mm" bind:__l="__l"></uni-dateformat></view></view><block wx:if="{{form.categoryId==19}}"><view class="escort-box"><block wx:if="{{timelist[timeIndex]}}"><view class="escort-time flexd jubetween"><text class="font28 fontColor-999"><block wx:if="{{form.service_item==1}}"><text>陪护</text></block><block wx:else><text>看病</text></block>时间</text><text class="font28">{{timelist[timeIndex].accompany_time}}</text></view></block><view class="escort-age flexd jubetween"><text class="font28 fontColor-999">陪护年龄</text><text class="font28">{{form.accompany_age}}</text></view><view class="escort-sex flexd jubetween"><text class="font28 fontColor-999">陪护性别</text><block wx:if="{{form.accompany_sex==0}}"><text class="font28">男</text></block><block wx:else><text class="font28">女</text></block></view><view class="escort-place flexd jubetween"><text class="font28 fontColor-999"><block wx:if="{{form.service_item==1}}"><text>陪护</text></block><block wx:else><text>看病</text></block>场所</text><text class="font28">{{form.accompany_place}}</text></view></view></block><block wx:if="{{form.categoryId==11}}"><view class="kitchen-box"><view class="kitchen-num flexd jubetween"><text class="font28 fontColor-999">菜品数量</text><text class="font28">{{form.dishes_number}}</text></view><view class="kitchen-type flexd jubetween"><text class="font28 fontColor-999">菜品</text><block wx:if="{{form.type==0}}"><text class="font28">自备</text></block><block wx:else><text class="font28">代买</text></block></view></view></block><block wx:if="{{form.categoryId==10}}"><view class="catdog-box flexd jubetween"><view class="catdog-title"><text class="font28 fontColor-999">宠物</text></view><view class="catdog-type font28"><text>{{form.category_name2}}</text></view></view></block></view><block wx:if="{{form.categoryId==19}}"><view class="escort-obj-box"><view class="obj-title"><text class="font28">服务对象</text></view><view class="obj-relationship flexd jubetween"><text class="font28 fontColor-999">与本人关系</text><text class="font28">{{form.service_object_relation}}</text></view><view class="obj-name flexd jubetween"><text class="font28 fontColor-999">姓名</text><text class="font28">{{form.service_object_name}}</text></view><view class="obj-sex flexd jubetween"><text class="font28 fontColor-999">性别</text><block wx:if="{{form.service_object_sex==0}}"><text class="font28">男</text></block><block wx:else><text class="font28">女</text></block></view><view class="obj-age flexd jubetween"><text class="font28 fontColor-999">年龄</text><text class="font28">{{form.service_object_age}}</text></view><view class="obj-symptom flexd jubetween"><text class="font28 fontColor-999">症状</text><text class="font28">{{form.service_object_symptom}}</text></view></view></block><view class="address-box"><view class="box-title"><block wx:if="{{form.categoryId==10}}"><text class="font28 fontColor-999"><block wx:if="{{form.handover_mode==0}}"><text>上门自取</text></block><block wx:else><text>指定位置</text></block></text></block><block wx:else><block wx:if="{{form.categoryId==19||form.categoryId==10}}"><text class="font28 fontColor-999"><block wx:if="{{form.handover_mode==0}}"><text>上门服务</text></block><block wx:else><text>指定地点</text></block></text></block><block wx:else><text class="font28 fontColor-999">上门地址</text></block></block></view><view class="address-news"><view class="news-contact flexd flex-center jubetween"><text class="font30">{{form.contact_name+" "+form.contact_phone}}</text><image class="icon32" src="/static/image/icon_phone.png" mode data-event-opts="{{[['tap',[['phone',['$0'],['form.contact_phone']]]]]}}" bindtap="__e"></image></view><view class="news-address flexd flex-center jubetween"><text class="font28 fontColor-666">{{form.address}}</text><image class="icon32" src="/static/image/icon_dw.png" mode></image></view></view></view><view class="remarks-box"><block wx:if="{{!form.remark}}"><text class="fontColor-999">没留下什么, 试试线上沟通~</text></block><block wx:else><text>{{form.remark}}</text></block></view></view>