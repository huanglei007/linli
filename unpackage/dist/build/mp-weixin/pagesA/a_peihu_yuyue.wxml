<view class="peihu-yuyue yuyue-box"><view class="peihu-yuyue-type"><view class="peihu-yuyue-type-title"><text class="font30">服务项目</text></view><view class="peihu-yuyue-type-select flex1-textRight"><picker value="{{index}}" range="{{typelist}}" range-key="category_name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><block wx:if="{{index==null}}"><text class="font28 fontColor-ccc">请选择</text></block><block wx:else><text class="font28">{{typelist[index].category_name}}</text></block><image class="icon22" src="/static/image/icon_gd.png" alt></image></view></picker></view></view><view class="peihu-yuyue-obj"><view class="peihu-yuyue-obj-title"><text class="font30">服务对象</text><block wx:if="{{objform.service_object_age}}"><image class="icon32" src="/static/img/icon_edit.png" mode data-event-opts="{{[['tap',[['subEvent',['$event']]]]]}}" bindtap="__e"></image></block></view><view class="peihu-yuyue-obj-add"><block wx:if="{{objform.service_object_name==''}}"><view data-event-opts="{{[['tap',[['subEvent',['$event']]]]]}}" class="addbtn" bindtap="__e"><text>+ 添加服务对象</text></view></block><block wx:else><view class="objbox" style="text-align:left;padding-left:30rpx;"><view class="name"><text>姓名：</text><text class="font28">{{objform.service_object_name}}</text></view><view class="age"><text>年龄：</text><text class="font28">{{objform.service_object_age}}</text></view><view class="sex"><text>性别：</text><text class="font28"><block wx:if="{{objform.service_object_sex==0}}"><text>男</text></block><block wx:else><text>女</text></block></text></view><view class="guanx"><text>关系：</text><text class="font28">{{objform.service_object_relation}}</text></view><view><text>症状：</text><text class="font28">{{objform.service_object_symptom}}</text></view></view></block></view></view><view class="peihu-yuyue-news"><view class="peihu-yuyue-news-age"><view class="peihu-yuyue-news-age-title"><text class="font30">陪护年龄</text></view><view class="peihu-yuyue-news-age-val flex1-textRight"><picker value="{{ageIndex}}" range="{{agelist}}" range-key="age" data-event-opts="{{[['change',[['bindPickerChange_age',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><block wx:if="{{ageIndex==null}}"><text class="font28 fontColor-ccc">请选择</text></block><block wx:else><text class="font28">{{agelist[ageIndex].age}}</text></block><image class="icon22" src="/static/image/icon_gd.png" alt></image></view></picker></view></view><view class="peihu-yuyue-news-gender"><view class="peihu-yuyue-news-gender-title"><text class="font30">陪护性别</text></view><view class="peihu-yuyue-news-gender-radio"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['radio1',(form.accompany_sex==0)?'news-active':'']}}" bindtap="__e">男</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['radio2',(form.accompany_sex==1)?'news-active':'']}}" bindtap="__e">女</text></view></view><view class="peihu-yuyue-news-place"><view class="peihu-yuyue-news-place-title"><text hidden="{{!(typelist[index].category_name=='陪护')}}" class="font30">陪护场所</text><text hidden="{{!(typelist[index].category_name=='看病')}}" class="font30">看病场所</text></view><view class="peihu-yuyue-news-place-val flex1-textRight"><block wx:if="{{typelist[index].category_name=='看病'}}"><input style="text-align:right;" type="text" placeholder="请输入具体地址"/></block><block wx:else><picker value="{{placeIndex}}" range="{{placelist}}" data-event-opts="{{[['change',[['bindPickerChange_place',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><block wx:if="{{placeIndex==null}}"><text class="font28 fontColor-ccc">请选择</text></block><block wx:else><text class="font28">{{placelist[placeIndex]}}</text></block><image class="icon22" src="/static/image/icon_gd.png" alt></image></view></picker></block></view></view></view><a-up-address vue-id="516e430e-1" upAddressId="{{upAddressId}}" bind:__l="__l"></a-up-address><a-up-time vue-id="516e430e-2" bind:__l="__l"></a-up-time><view class="peihu-yuyue-time"><view class="peihu-yuyue-time-title"><text hidden="{{!(typelist[index].category_name=='陪护')}}" class="font30">陪护时间</text><text hidden="{{!(typelist[index].category_name=='看病')}}" class="font30">看病时间</text></view><view class="peihu-yuyue-time-select flex1-textRight"><picker value="{{timeIndex}}" range="{{timelist}}" range-key="accompany_time" data-event-opts="{{[['change',[['bindPickerChange_time',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><block wx:if="{{timeIndex==null}}"><text class="font28 fontColor-ccc">请选择</text></block><block wx:else><text class="font28">{{timelist[timeIndex].accompany_time}}</text></block><image class="icon22" src="/static/image/icon_gd.png" alt></image></view></picker></view></view><a-discount vue-id="516e430e-3" type="{{6}}" price="{{price}}" bind:__l="__l"></a-discount><a-remarks vue-id="516e430e-4" bind:__l="__l"></a-remarks><a-submit vue-id="516e430e-5" type="陪护看病" form="{{form}}" bind:__l="__l"></a-submit></view>