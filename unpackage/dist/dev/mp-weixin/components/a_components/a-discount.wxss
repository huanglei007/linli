.baojie-yuyue-disc {
  padding: 30rpx 40rpx;
  border-radius: 20rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.baojie-yuyue-disc .baojie-yuyue-disc-select {
  text-align: right;
  margin-left: 20rpx;
}
.pop-box {
  padding: 30rpx;
}
.pop-box .list {
  flex: 1;
  position: relative;
  line-height: 40rpx;
  margin: 0 15rpx 0 0;
  padding: 20rpx 0;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  border-radius: 28rpx;
}
.pop-box .list .list-left {
  width: 35%;
  color: #FF8740;
  text-align: center;
}
.pop-box .list .list-right {
  width: 60%;
  margin-left: 5%;
}
.status {
  position: absolute;
  top: 0;
  left: 0;
  padding: 0 10rpx;
  border-radius: 28rpx 0 28rpx 0;
  background-color: #FF8740;
}
.status text {
  font-size: 24rpx;
  color: #fff;
}

