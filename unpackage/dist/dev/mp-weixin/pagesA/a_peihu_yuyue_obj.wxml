<view class="obj yuyue-box"><view class="obj-relation"><view class="obj-relation-title"><text class="font30">与本人关系</text></view><view class="obj-relation-select flex1-textRight"><picker value="{{index}}" range="{{relationlist}}" range-key="relation" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><block wx:if="{{index==null}}"><text class="font28 fontColor-ccc">请选择</text></block><block wx:else><text class="font28"><text>{{relationlist[index].relation}}</text></text></block><image class="icon22" src="/static/image/icon_gd.png" mode></image></view></picker></view></view><view class="obj-name"><view class="obj-name-title"><text class="font30">姓名</text></view><view class="obj-name-input flex1-textRight"><input style="text-align:right;" type="text" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','service_object_name','$event',[]],['form']]]]]}}" value="{{form.service_object_name}}" bindinput="__e"/></view></view><view class="obj-gender"><view class="obj-gender-title"><text class="font30">性别</text></view><view class="obj-gender-radio"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['radio1',(form.service_object_sex==0)?'news-active':'']}}" bindtap="__e">男</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['radio2',(form.service_object_sex==1)?'news-active':'']}}" bindtap="__e">女</text></view></view><view class="obj-age"><view class="obj-age-title"><text class="font30">年龄</text></view><view class="obj-age-input flex1-textRight"><input style="text-align:right;" type="number" placeholder="请输入年龄" data-event-opts="{{[['input',[['__set_model',['$0','service_object_age','$event',[]],['form']]]]]}}" value="{{form.service_object_age}}" bindinput="__e"/></view></view><view class="obj-symptom"><view class="obj-symptom-title"><text class="font30">症状</text></view><view class="obj-symptom-textarea"><textarea placeholder-style="color:#CCCCCC" placeholder="补充描述症状" data-event-opts="{{[['input',[['__set_model',['$0','service_object_symptom','$event',[]],['form']]]]]}}" value="{{form.service_object_symptom}}" bindinput="__e"></textarea></view></view><view class="sub"><view data-event-opts="{{[['tap',[['subEvent',['$event']]]]]}}" class="sub-btn" bindtap="__e"><text>提交</text></view></view></view>