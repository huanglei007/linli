<view><view class="typeBox flexd jucenter"><block wx:for="{{typeList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({i})}}" class="{{['item',typeIndex==i?'active':'']}}" bindtap="__e">{{''+item+''}}</view></block></view><view class="list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="{{['item',item.isDelect==1?'hide':'']}}"><view class="flexd juend"><image class="jian" src="/static/image/icon_jian.png" mode="widthFix" data-event-opts="{{[['tap',[['del',[i]]]]]}}" bindtap="__e"></image></view><view class="itemBox flexd jucenter flex-center"><view class="input"><input type="text" placeholder="{{typeIndex==0?'请输入现价':'请输入消费满额'}}" data-event-opts="{{[['input',[['__set_model',['$0','left','$event',[]],[[['list','',i]]]]]]]}}" value="{{item.left}}" bindinput="__e"/></view><view class="center">{{''+(typeIndex==0?'抵':'减')+''}}</view><view class="input"><input type="text" placeholder="{{typeIndex==0?'请输入原价':'请输入减额'}}" data-event-opts="{{[['input',[['__set_model',['$0','right','$event',[]],[[['list','',i]]]]]]]}}" value="{{item.right}}" bindinput="__e"/></view></view><view class="itemBox flexd jucenter flex-center"><picker class="input" mode="date" value="{{item.begin}}" data-event-opts="{{[['change',[['bindDateChange',['$event','$0','begin'],[[['list','',i]]]]]]]}}" bindchange="__e"><view class="uni-input">{{item.begin}}</view></picker><view class="center">-</view><picker class="input" mode="date" value="{{item.end}}" data-event-opts="{{[['change',[['bindDateChange',['$event','$0','end'],[[['list','',i]]]]]]]}}" bindchange="__e"><view class="uni-input">{{item.end}}</view></picker></view></view></block><view class="item flexd jucenter"><image class="add" src="/static/image/icon_add.png" mode="widthFix" data-event-opts="{{[['tap',[['newArr',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="foot"><view data-event-opts="{{[['tap',[['$shake',['$0'],['submit']]]]]}}" class="btn" bindtap="__e">提交审核</view></view></view>